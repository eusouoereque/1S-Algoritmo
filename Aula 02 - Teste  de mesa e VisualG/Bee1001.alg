algoritmo "Bee1001"
// Função :
// Autor :
// Data : 06/03/2023
// Seção de Declarações 
var
   a, b, soma : inteiro
inicio
// Seção de Comandos 

   escreval("Digite o primeiro numero: ")
   leia(a)
   
   escreval("Digite o segundo numero: ")
   leia(b)
   
   soma <- a + b
   
   escreval("O resultado da soma é: ", soma)
   
fimalgoritmo
